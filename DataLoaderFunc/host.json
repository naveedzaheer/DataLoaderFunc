{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:01",
      "visibilityTimeout": "00:00:30",
      "batchSize": 32,
      "maxDequeueCount": 5,
      "newBatchThreshold": 8,
      "messageEncoding": "base64"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    }
  }
}